mixin header(menuItems, avatarSrc)
	header.header
		.header-top#header-top
			.container
				.header-content
					.header-nav
						.header-nav__row
							.header-slogan
								p Свежие цветы со всего мира!
							.search-panel
								.input-wraper
									input(type="text" placeholder="Поиск...")
									button.search-button
										+search
							+social-icons
							.contact-phone
								a(href="tel:+79055555555")
									span(itemprop="telephone")  +7 905 555-55-55
						.header-nav__row.mb-mobile
							a(href="index.html").logo
								span.red-text M
								span flowers
							a(href="index.html").logo.logo-small
								img(src="img/main/logo.png", alt="logo")
							ul.header-menu
								each item in menuItems
									if item.addClass
										li(class=" "+ item.addClass  )
											a(href=" "+ item.hrefLink) #{item.text}
									else if item.text == 'Каталог'
										li.drop-item
											a(href=" "+ item.hrefLink) #{item.text}
											.drop-menu
												.drop-menu-wrapper
													.acordion-list
														.acordion-list-title
															a(href=" "+ item.hrefLink) Классические
															span.open-list +
														.acordion-list-items
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы
													.acordion-list
														.acordion-list-title
															a(href=" "+ item.hrefLink) Классические
															span.open-list +
														.acordion-list-items
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы
													.acordion-list
														.acordion-list-title
															a(href=" "+ item.hrefLink) Классические
															span.open-list +
														.acordion-list-items
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы
													.acordion-list
														.acordion-list-title
															a(href=" "+ item.hrefLink) Классические
															span.open-list +
														.acordion-list-items
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы
															a(href="#!") Розы

									else
										li
											a(href=" "+ item.hrefLink) #{item.text}
								.header-menu__contacts
									.contact-phone
										a(href="tel:+79055555555")
											span(itemprop="telephone")  +7 905 555-55-55
									+social-icons

								.menu-social
									+social-icons
							.user-controls
								if avatarSrc
									.user-avatar
										span
										a(href="#" style="background-image: url(img/main/icons/"+  avatarSrc +".jpg), url(img/main/icons/noavatar.jpg) ")

								else
									.user-log-in
										a(href="user-profile.html")
											+log-in

								.user-favor
									a(href="#!")
										+favor
										span.favor-value 2
								.user-basket
									a(href="page-basket.html")
										+cart-icon
										span.basket-value 2
							.menu-toggle
								span
		.cat-panel#catPanel
			.container
				.category-menu
					nav
						a(href="#") Овощи
						a(href="#") Фрукты
						a(href="#") Экзотические фрукты
						a(href="#") Мясо, рыба, птица
						a(href="#") Вода, напитки, соки
						a(href="#") Орехи и сухофрукты

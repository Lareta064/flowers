mixin radioBlock( itemId,text1,text2, radioName,  text3, check, addPrice)
	.form-group&attributes(attributes)
		if check
			input(type="radio" id= itemId name= radioName checked)
		else
			input(type="radio" id= itemId name= radioName )
		label(for= itemId)
			span !{text1}
			span !{text2}
			span !{text3}
		if addPrice
			span.addprice !{addPrice}

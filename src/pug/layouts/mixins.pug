mixin main-product-card(title, imgSrc, weight, price, oldPrice,desk )
	.product-card-item
		div(itemscope itemtype="http://schema.org/Product")
		.card-badges
			.card-badge.card-badge--discount 12%
			.card-badge.card-badge--new New
			.card-badge.card-badge--hit Хит
		//===== social-icons =====
		.icon-group
			a(href="#!")
				+icon-heart
			a(href="#!").ic-cart
				+cart-icon
			a(href="#!")
				+view
		//===== badges =====
		
		//===== card-body =====
		a(href="product-card.html").product-card&attributes(attributes)
			.product-card__img
				img(itemprop="image" src="img/products/"+imgSrc+".jpg"  srcset="img/products/"+imgSrc+".jpg 2x" alt="img" width=""+ weight)
			.product-card__title
				h3(itemprop="name") !{title}
				.price-block
					p(itemprop="price" content=" "+ price ) #{price} руб.
					if oldPrice
						p.oldPrice #{oldPrice} руб.
				if desk
					.product-card__desk(itemprop="description")
						p Cостав:
						p #{desk}

mixin priceBlock(priceVal, oldPrice)
	.price-block&attributes(attributes)
		p(itemprop="price" content=" "+ priceVal ) #{priceVal} руб.
		if oldPrice
			p.oldPrice #{oldPrice} руб.

